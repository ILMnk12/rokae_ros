      </geometry>
       <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
       <material name="white"/>
    </visual>
    <inertial>
      
  <mass value="5.48267"/>
  <inertia ixx="0.056" ixy="0" ixz="0" iyy="0.066" iyz="0" izz="0.035"/>
